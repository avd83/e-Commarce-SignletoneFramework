<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="e-Commarce Automation Test Suite" parallel="tests" thread-count="2">
	
<listeners>
<listener class-name="com.qa.base.Listeners"/>
</listeners>

 <test name="Login Test Cases" preserve-order="true">
	<parameter name="browser" value="chrome"/>
    <classes>
		<class name="com.qa.tests.LoginTest"/>
	</classes>
 </test>  
 <test name="HomePage Test Cases" preserve-order="true">
	<parameter name="browser" value="firefox"/>
    <classes>
		<class name="com.qa.tests.HomePageTest"/>
	</classes>
 </test>
</suite> 
